{
  "expo": {
    "name": "Yet Another Movie App",
    "slug": "YAMA",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#141B31"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.karanwadhwa.YAMA",
      "versionCode": 1,
      "permissions": [""]
    },
    "androidStatusBar": {
      "backgroundColor": "#141B31"
    },
    "description": "ReactNative App to discover movies and TV shows using the TMDb API",
    "githubUrl": "https://github.com/karanwadhwa/YetAnotherMovieApp"
  }
}
